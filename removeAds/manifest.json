{
  "name": "OutlookAdBlocker",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://outlook.live.com/mail/*"], //where your script should be injected
      "css": ["maincss.css"], //the name of the file to be injected
      "js": ["content.js", "jquery.js"]
    }
  ]
}